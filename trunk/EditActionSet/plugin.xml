<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension point="org.eclipse.ui.actionSets">
		<actionSet label="JottingUML" visible="true"
				id="org.kotemaru.eclipse.EditActionSet.ActionSet">

		    <action label="paste" 
				icon="icons/paste.png"
				toolbarPath="EditActionSet"
				tooltip="Paste"
				retarget="true"
				id="paste"
			></action>
		    <action label="copy" 
				icon="icons/copy.png"
				toolbarPath="EditActionSet"
				tooltip="Copy"
				retarget="true"
				id="copy"
			></action>
		    <action label="cut" 
				icon="icons/cut.png"
				toolbarPath="EditActionSet"
				tooltip="Cut"
				retarget="true"
				id="cut"
			></action>


		    <action label="redo"
				icon="icons/redo.png"
				toolbarPath="EditActionSet"
				tooltip="REDO"
				retarget="true"
				id="redo"
			></action>
		    <action label="undo"
				icon="icons/undo.png"
				toolbarPath="EditActionSet"
				tooltip="UNDO"
				retarget="true"
				id="undo"
			></action>
		</actionSet>
	</extension>


</plugin>
